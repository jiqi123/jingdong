        function unifyHeight(thsClass,unifyHei){
            var thsClassSpan = thsClass.find(".operating-floor-txt span.second");
            thsClassSpan.each(function(i){
                if($(this).height() > unifyHei){
                    unifyHei = $(this).height();
                }
            });
            thsClassSpan.height(unifyHei);
        }
        //运营楼层
        if($("#isShowActivityLayer").val() === 'true'){
            var locations = '/cart/activityLayer.action';
            jQuery.get(locations,function(data0){
                if(data0!=null){
                    $(".operating-floor-container").append(data0);
                    if($(".operating-floor-one").length > 0){
                        $(".operating-floor-only").height($(".operating-floor-only").width() / 3.57);
                    }
                    if($(".operating-floor-three").length > 0){
                        unifyHeight($(".operating-floor-three"),$(".operating-floor-three").find(".operating-floor-txt span").height());
