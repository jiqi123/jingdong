        var getSid = function(){
            return {
                fromCookie:function(){
                    var arr,reg=/(^| )sid=([^;]*)(;|$)/;
                    if(arr=document.cookie.match(reg)) return unescape(arr[2]);else return null;
                },
                fromQueryString:function(){
                    var reg = /(^|&)sid=([^&]*)(&|$)/;
                    var r = window.location.search.substr(1).match(reg);
                    if(r!=null)return  unescape(r[2]); return null;
                }
            };
        }();
        var sid_qs = getSid.fromQueryString();
        var sid_ck = getSid.fromCookie();
        if(sid_ck && sid_qs){
            if(sid_ck != sid_qs){
                var strSearch = window.location.search;
                var newStrSearch = strSearch.replace(/(^|\?|&)(sid=)([0-9a-zA-Z]*)(&|$)/,'$1$2'+sid_ck+'$4');
